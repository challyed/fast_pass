{
  "name": "lv-dof",
  "description": "Las Vegas Development Opportunity Finder",
  "version": "0.0.1",
  "private": true,
  "repository": {
      "type": "git",
      "url": "https://github.com/rclosner/lv-dof"
  },
  "scripts": {
    "start": "node app.js",
    "migrate-db": "db-migrate up --config config/database.json --migrations-dir db/migrations -e \"$NODE_ENV\"",
    "rollback-db": "db-migrate down --config config/database.json --migrations-dir db/migrations -e \"$NODE_ENV\"",
    "create-db": "psql -c \"CREATE DATABASE fast_track_$NODE_ENV;\""
  },
  "engines": {
    "node": "0.10.x",
    "npm": "1.2.x"
  },
  "dependencies": {
    "express": "3.2.6",
    "pg": "2.2.0",
    "jade": "*",
    "less-middleware": "0.1.x",
    "proj4js": "*"
  }
}
